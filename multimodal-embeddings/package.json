{
	"name": "mm-embeddings",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:emulate": "concurrently -n 'mmembed,firebase' -c 'blue,yellow' 'USE_EM=true vite dev' 'npm run emulate'",
		"emulate": "firebase emulators:start --import=emulator-export",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"deploy": "vite build && gcloud app deploy --version dev build/app.yaml --no-promote --quiet",
		"deploy:prod": "vite build && gcloud app deploy --version prod build/app.yaml"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint": "^8.56.7",
		"@types/three": "^0.165.0",
		"autoprefixer": "^10.4.19",
		"concurrently": "^9.0.1",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"svelte": "^4.2.19",
		"svelte-adapter-appengine": "^1.1.0",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3"
	},
	"type": "module",
	"browserify": {
		"transform": [
			"cwise"
		]
	},
	"dependencies": {
		"@google-cloud/aiplatform": "^3.23.0",
		"@google-cloud/firestore": "^7.8.0",
		"@google-cloud/vertexai": "^1.2.0",
		"@tensorflow/tfjs-tsne": "^0.2.0",
		"@threlte/core": "^7.3.0",
		"@threlte/extras": "^8.11.2",
		"bits-ui": "^0.21.10",
		"clsx": "^2.1.1",
		"dat.gui": "^0.7.9",
		"dotenv": "^16.4.5",
		"firebase": "^10.12.2",
		"lucide-svelte": "^0.396.0",
		"sharp": "^0.33.4",
		"svelte-radix": "^1.1.0",
		"svelte-tweakpane-ui": "^1.3.0",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"three": "^0.165.0",
		"umap-js": "^1.4.0"
	}
}
